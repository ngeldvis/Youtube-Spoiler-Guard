{
    "name": "Youtube Spoiler Guard",
    "author": "Nigel Davis",
    "description": "chrome extension that attempts to hide spoilers on youtube",
    "version": "1.0",
    "manifest_version": 3,
    "options_page": "options.html",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "https://www.youtube.com/"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}